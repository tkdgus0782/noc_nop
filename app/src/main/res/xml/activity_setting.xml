<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">


    <CheckBoxPreference
        android:key="checkbox1"
        android:title="낮은모델 사용"
        android:summary="정확도를 내리고 처리속도를 올립니다"
        android:dependency="switch1"/>
    <CheckBoxPreference
        android:key="checkbox2"
        android:title="야간모드 사용"
        android:summary="주변이 어두우면 플래시를 활성화합니다"
        android:defaultValue="true"/>
    <CheckBoxPreference
        android:key="checkbox3"
        android:title="진동모드 사용"
        android:summary="알림을 줄 때 진동또한 줍니다"
        android:defaultValue="true"/>
    <SwitchPreference
        android:key="switch1"
        android:title="연구실"
        android:summary="개발자용 옵션입니다" />

    <CheckBoxPreference
        android:key="checkbox4"
        android:title="높은모델 사용"
        android:summary="정확도를 올리고 처리속도를 내립니다"
        android:enabled="false"
        android:dependency="switch1"
        />
    <CheckBoxPreference
        android:key="checkbox5"
        android:title="저전력모드"
        android:summary="전력소모를 낮추고 성능을 낮춥니다"
        android:enabled="false"
        android:dependency="switch1"
        />
    <CheckBoxPreference
        android:key="checkbox6"
        android:title="로그표출"
        android:summary="라이브화면에서 로그를 띄웁니다"
        android:enabled="false"
        android:defaultValue="true"
        android:dependency="switch1"
        />
</PreferenceScreen>